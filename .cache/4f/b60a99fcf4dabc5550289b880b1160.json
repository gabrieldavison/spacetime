 auto;\n  color: var(--color-dark);\n  background-color: var(--color-light);\n  font-family: \"IBM Plex Mono\", monospace;\n}\n\n/* Styles help modal and associated buttons */\n\n#help-button {\n  position: absolute;\n  left: 94vw;\n  border: 2px solid var(--color-dark);\n  padding: 0.2em 0.5em 0.2em 0.5em;\n  text-align: right;\n  font-size: 2em;\n}\n\n#help-button:hover {\n  color: var(--color-light);\n  background-color: var(--color-dark);\n}\n#help-modal {\n  position: absolute;\n  z-index: 100;\n  top: 10rem;\n  left: 0;\n  right: 0;\n  max-width: 750px;\n  padding: 3em;\n  margin-left: auto;\n  margin-right: auto;\n  color: var(--color-light);\n  background-color: var(--color-dark);\n  font-size: 1.2em;\n}\n\n#help-modal li {\n  list-style-type: none;\n}\n#close-modal-container {\n  text-align: right;\n}\n\n#close-modal-button {\n  border: 2px solid var(--color-light);\n  padding: 0.2em 0.5em 0.2em 0.5em;\n}\n#close-modal-button:hover {\n  color: var(--color-dark);\n  background-color: var(--color-light);\n}\n\n/* Styles preformatted header */\n\npre {\n  margin-left: 4rem;\n  margin-top: 3rem;\n  font-size: 0.3em;\n  font-weight: bold;\n}\n\n.container {\n  position: relative;\n  width: 800px;\n  height: 200px;\n  margin: auto;\n  text-align: center;\n}\n\n/* Styles sequencers and cofigure screean */\n\ncanvas {\n  position: relative;\n}\n\n#configure-modal {\n  position: absolute;\n  top: 0;\n  left: 0;\n  width: 800px;\n  height: 250px;\n  background-color: var(--color-light);\n  opacity: 0.7;\n}\n\n.codeflask__textarea {\n  background-color: var(--color-light) !important;\n}\n\n#error-display {\n  height: 30px;\n  margin: 0;\n}\n\n.hidden {\n  display: none;\n}\n.visible {\n  display: block;\n}\n.dimmed {\n  opacity: 0.2;\n}\n\n/* Engine Styling */\n\n#start-sound {\n  display: block;\n  border: 2px solid var(--color-dark);\n  padding: 0.3em;\n  margin: 1em auto 0 auto;\n  outline: none;\n  font-size: 1.2em;\n  background-color: var(--color-light);\n}\n#start-sound:hover {\n  background-color: var(--color-dark);\n  color: var(--color-light);\n}\n\n.synth-controls {\n  display: flex;\n  flex-direction: row;\n  flex-wrap: wrap;\n  justify-content: center;\n  align-items: flex-start;\n}\n\n.control-block {\n  flex-grow: 1;\n  flex: 1;\n  max-width: 350px;\n  padding: 1em;\n  margin: 0.5em;\n  margin-bottom: 15px;\n  border: 2px solid var(--color-dark);\n  font-size: 1.2em;\n  text-align: center;\n}\n.control {\n  text-align: center;\n}\n\n.control-block span {\n  display: block;\n  color: var(--light-color);\n}\n\n.toggle-container {\n  display: flex;\n  flex-direction: row;\n  justify-content: space-evenly;\n  max-width: 500px;\n  align-items: center;\n  margin: auto;\n}\n.toggle-control {\n  width: 33%;\n  margin-top: 1em;\n  text-align: center;\n  font-size: 0.8em;\n}\nwebaudio-switch {\n  border: 2px solid var(--color-dark);\n}\n\nwebaudio-slider {\n  border: 2px solid var(--color-dark);\n}\n"},"lineCount":null}},"error":null,"hash":"878d60871822d76e0f9a1a8054a5d90a","cacheData":{"env":{}}}